<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<project basedir="." default="build" name="gamePart">
	<!--// import //-->
	<import file="../../all_config/antDef/ant-jarOneProj.xml" />

	<!--// 目标项目 //-->
	<path id="targetProj" location="${basedir}/.." />

	<!--// 相关项目 //-->
	<path id="relativeProjArr">
	</path>

	<!--// 引用包目录 //-->
	<path id="libRef">
		<path refid="code_lib.common" />
    </path>

	<!--// 构建当前项目 //-->
	<target name="build">
		<jarOneProj targetProj="targetProj" targetVer="${targetVer}" 
			libRef="libRef" 
			relativeProjArr="relativeProjArr" 
		/>
	</target>
</project>
