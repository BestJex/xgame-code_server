<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<project basedir="." default="build" name="passportServer">
	<!--// import //-->
	<import file="../../all_config/antDef/ant-jarOneProj.xml" />

	<!--// 目标项目 //-->
	<path id="targetProj" location="${basedir}/.." />

	<!--// 相关项目 //-->
	<path id="relativeProjs">
	</path>

	<!--// 引用包目录 //-->
	<path id="libs">
		<path refid="code_lib.common" />
		<path refid="code_lib.jetty" />
    </path>

	<!--// 构建当前项目 //-->
	<target name="build">
		<jarOneProj targetProj="targetProj" targetVer="${targetVer}" 
			libs="libs" 
			relativeProjs="relativeProjs" />
	</target>
</project>
